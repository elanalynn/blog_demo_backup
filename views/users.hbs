<h1>{{title}}</h1>
<p>Welcome to the {{title}}</p>

{{#each users}}
  <a href="/users/{{this.postId}}"><h3>{{this.title}}</h3></a>
  <div class="">
    <img class="user-image circle" src="{{this.userImage}}" alt="{{this.title}}" />
    <p>By {{this.first_name}} {{this.last_name}}</p>
  </div>
  <img class="post-image" src="{{this.image_url}}" alt="{{this.title}}" />
  <p>{{this.body}}</p>
{{/each}}
